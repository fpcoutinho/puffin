{
  "name": "puffin",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.app.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@fontsource-variable/lora": "^5.0.9",
    "@fontsource-variable/merriweather-sans": "^5.0.9",
    "@fontsource-variable/montserrat": "^5.0.16",
    "@fontsource-variable/nunito": "^5.0.16",
    "@fontsource-variable/nunito-sans": "^5.0.9",
    "@fontsource-variable/open-sans": "^5.0.18",
    "@fontsource/blinker": "^5.0.17",
    "@fontsource/julius-sans-one": "^5.0.17",
    "@fontsource/la-belle-aurore": "^5.0.17",
    "@fontsource/libre-baskerville": "^5.0.8",
    "@fontsource/material-symbols-outlined": "^5.0.16",
    "@fontsource/yellowtail": "^5.0.17",
    "axios": "^1.6.1",
    "sass": "^1.69.5",
